{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "35c2a030-70e4-44b0-bcac-3c33ef95e5a5"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "velzon": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/velzon",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              {
                "glob": "**/*",
                "input": "src/assets/viewer",
                "output": "/assets/viewer"
              }
            ],
            "styles": [
              "node_modules/prismjs/themes/prism.css",
              "node_modules/shepherd.js/dist/css/shepherd.css",
              "node_modules/flatpickr/dist/flatpickr.css",
              "src/styles.scss",
              "node_modules/slick-carousel/slick/slick.scss",
              "node_modules/slick-carousel/slick/slick-theme.scss",
              "node_modules/@ctrl/ngx-emoji-mart/picker.css",
              "node_modules/select2/dist/css/select2.min.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
              "node_modules/bootstrap-icons/font/bootstrap-icons.min.css",
              "src/assets/scss/config/default/bootstrap.scss",
              "src/assets/scss/config/default/app.scss",
              "src/assets/scss/icons.scss",
              "node_modules/datatables.net-dt/css/dataTables.dataTables.min.css",
              "src/assets/dataTable/datatables.net-bs5/css/dataTables.bootstrap5.min.css",
              "src/assets/dataTable/datatables.net-responsive-bs5/css/responsive.bootstrap5.min.css",
              "src/assets/dataTable/datatables.net-fixedcolumns-bs5/css/fixedColumns.bootstrap5.min.css",
              "src/assets/dataTable/datatables.net-fixedheader-bs5/fixedHeader.bootstrap5.min.css",
              "src/assets/dataTable/datatables.net-buttons-bs5/css/buttons.bootstrap5.min.css",
              "src/assets/dataTable/datatables.net-select-bs5/select.bootstrap5.min.css",
              "src/assets/glightbox/css/glightbox.min.css",
              "node_modules/select2/dist/css/select2.min.css"
              
            ],
            "scripts": [
              "node_modules/jquery/dist/jquery.min.js",
              "node_modules/slick-carousel/slick/slick.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.min.js",
              "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
              "node_modules/select2/dist/js/select2.min.js",
              "node_modules/datatables.net/js/dataTables.min.js",
              "src/assets/dataTable/datatables.net/jquery.dataTables.min.js",
              "src/assets/dataTable/datatables.net-bs5/js/dataTables.bootstrap5.min.js",
              "src/assets/dataTable/datatables.net-responsive/dataTables.responsive.min.js",
              "src/assets/dataTable/datatables.net-responsive-bs5/js/responsive.bootstrap5.min.js",
              "src/assets/dataTable/datatables.net-fixedcolumns-bs5/js/fixedColumns.bootstrap5.min.js",
              "src/assets/dataTable/datatables.net-fixedheader/dataTables.fixedHeader.min.js",
              "src/assets/dataTable/datatables.net-buttons/dataTables.buttons.min.js",
              "src/assets/dataTable/datatables.net-buttons-bs5/js/buttons.bootstrap5.min.js",
              "src/assets/dataTable/datatables.net-buttons/buttons.html5.min.js",
              "src/assets/dataTable/datatables.net-buttons/buttons.flash.min.js",
              "src/assets/dataTable/datatables.net-buttons/buttons.print.min.js",
              "src/assets/dataTable/datatables.net-keytable/dataTables.keyTable.min.js",
              "src/assets/dataTable/datatables.net-select/dataTables.select.min.js",
              "src/assets/glightbox/js/glightbox.min.js",
              "node_modules/html2pdf.js/dist/html2pdf.min.js",
              "node_modules/jszip/dist/jszip.min.js",
              "node_modules/datatables.net-buttons/js/buttons.colVis.min.js",
              "node_modules/file-saver/dist/FileSaver.min.js"
            ],
            "allowedCommonJsDependencies": [
              "ev-emitter/ev-emitter",
              "can-use-dom",
              "core-js/modules/es.array.filter.js",
              "core-js/modules/es.array.iterator.js",
              "core-js/modules/es.array.reduce.js",
              "core-js/modules/es.function.name.js",
              "core-js/modules/es.object.assign.js",
              "core-js/modules/es.object.to-string.js",
              "core-js/modules/es.parse-int.js",
              "core-js/modules/es.regexp.exec.js",
              "core-js/modules/es.string.iterator.js",
              "core-js/modules/es.string.match.js",
              "core-js/modules/es.string.replace.js",
              "core-js/modules/es.weak-map.js",
              "core-js/modules/web.dom-collections.iterator.js",
              "lodash.debounce",
              "lodash.memoize",
              "lodash.throttle",
              "rxjs/internal/operators",
              "moment",
              "lottie-web",
              "ngx-lightbox",
              "ngx-masonry",
              "leaflet",
              "sweetalert2",
              "lodash",
              "@ckeditor/ckeditor5-build-classic",
              "ngx-csv/ngx-csv",
              "dropzone",
              "file-saver",
              "prismjs",
              "apexcharts",
              "canvg",
              "raf",
              "rgbcolor",
              "core-js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "8mb",
                  "maximumError": "8mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/app/environment/environment.ts",
                  "with": "src/app/environment/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                }
              }
            },
            "siaV1": {
              "fileReplacements": [
                {
                  "replace": "src/app/environment/environment.ts",
                  "with": "src/app/environment/environmentSiaV1.ts"
                }
              ],
              "outputHashing": "all",
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                }
              }
            },
            "siaV2": {
              "fileReplacements": [
                {
                  "replace": "src/app/environment/environment.ts",
                  "with": "src/app/environment/environmentSiaV2.ts"
                }
              ],
              "outputHashing": "all",
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": false
                }
              }
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },

        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "velzon:build:production"
            },
            "development": {
              "buildTarget": "velzon:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "velzon:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  }
}